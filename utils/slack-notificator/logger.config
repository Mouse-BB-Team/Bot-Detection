[loggers]
keys=root

[handlers]
keys=consoleHandler

[formatters]
keys=loggerFormatter

[logger_root]
level=DEBUG
handlers=consoleHandler

[logger_SlackNotifier]
level=DEBUG
handlers=consoleHandler

[handler_consoleHandler]
class=StreamHandler
level=DEBUG
formatter=loggerFormatter
args=(sys.stdout,)

[formatter_loggerFormatter]
format=%(asctime)s [%(name)-13.13s] [%(levelname)-4.4s]  %(message)s