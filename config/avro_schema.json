{
    "type": "record",
    "name": "userEvents",
    "namespace": "BotDetectionData",
    "doc": "Mouse events schema",
    "fields": [
        { "name": "userId", "type": "int", "order": "ignore" },
        { "name": "sequenceId", "type": "int", "order": "ignore" },
        { "name": "events", "order": "ignore", "type": {
            "type": "array",
            "items": {
                "type": "record",
                "order": "ignore",
                "name": "event",
                "fields": [
                    { "name": "id", "type": ["null", "int"], "order": "ascending"},
                    { "name": "xCoordinate", "type": "int", "order": "ignore"},
                    { "name": "yCoordinate", "type": "int", "order": "ignore" },
                    { "name": "xResolution", "type": "int", "order": "ignore"},
                    { "name": "yResolution", "type": "int", "order": "ignore"}
                ]
            }
        }},
        { "name": "sequenceLength", "type": "int", "order": "ignore"}
    ]
}
